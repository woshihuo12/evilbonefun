MobsMovement = 1		    #怪是否主动攻击     0=Disabled 1=Enabled
CloseAttackRadius = 30      #停止攻击的半径
RangeAttackRadius = 120     #攻击范围半径
DemolitionModifier = 0.5	#0.5 = 50%
PlayerMaxLevel = 400		#人物的最大等级,为0为不限制
InventoryLimeLimit = 2100000000		#lime
WarehouseLimeLimit = 2100000000		#lime

#Enhanced Exp Config
ExpPlayerMobDifference = 150 #人物与怪等级相差100级没有经验
ExpLowerStep = 1             #

#新手补偿
NewCharLevel = 299 #249级前为新人补偿阶段
Newcharxp = 4         #补偿经验倍数

#经验双倍活动时间段
StartTime = 8     #开始时间，为0没有活动
EndTime = 11
startxp = 2

#服务器最大登录人数
maxPlayer = 3000

