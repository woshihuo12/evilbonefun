#Server configuration object
[Server]
DataPath=classpath:/data/file
xp = 10       	#经验倍数  1=1x, 2=2x, 3=3x...
lime = 10			#金钱倍数  1=1x, 2=2x, 3=3x...
DefaultMap = 4      #默认地图
StartLime = 20160  #开始金钱
Version = 770       #版本
WelcomeMsg = 猴年大吉大利，财源滚滚  
SessionRadius = 300         #视线范围
MobRadiusArea = 50         #怪活动半径区域
DropExclusivity = 5        #掉落物品他人可以拾取的时间
DropTimeOut = 60           #掉落物品超时
SpawnAttempts = 3           #
CriticalMultiplier = 0		# 人物暴率系数 1 = 100% 两倍
CriticalChance = 0.0		# 人物暴击控制，值越大暴击机率越大，设为1 100%暴击 1 = 100%
PetEquipmentCompensation = 0.3	#宝宝装备补偿   0.3 = 30%
PetBreedTime = 7200       #宝宝出生时间，单位秒，72000秒=20小时
